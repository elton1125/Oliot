<Window x:Class="Harjoitus_19.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="300" Width="500">
    <Grid Margin="0,0,-32,-6">
        <Frame x:Name="Frame_Main" HorizontalAlignment="Left" VerticalAlignment="Top" Height="200" Width="300"/>
        <StackPanel HorizontalAlignment="Left" Height="239" Margin="20,20,0,0" VerticalAlignment="Top" Width="400">
            <TextBlock x:Name="tb_Kerros" Text="Kerros" Height="30" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,10,0,0" />
            <Button x:Name="btn_Kerros1" Content="1" Margin="10" Click="btn_Kerros_Click" Tag="1"/>
            <Button x:Name="btn_Kerros2" Content="2" Margin="10" Click="btn_Kerros_Click" Tag="2"/>
            <Button x:Name="btn_Kerros3" Content="3" Margin="10" Click="btn_Kerros_Click" Tag="3"/>
            <Button x:Name="btn_Kerros4" Content="4" Margin="10" Click="btn_Kerros_Click" Tag="4"/>
            <Button x:Name="btn_Kerros5" Content="5" Margin="10" Click="btn_Kerros_Click" Tag="5"/>
        </StackPanel>
    </Grid>
</Window>
